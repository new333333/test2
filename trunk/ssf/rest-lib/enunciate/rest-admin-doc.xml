<enunciate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="http://enunciate.webcohesion.com/schemas/enunciate-2.3.0.xsd" path-sort-strategy="depth_first">
    <title>Filr Admin REST Interface</title>
    <description>Reference Documentation for the Filr Administration REST Interface</description>
    <copyright>Micro Focus International plc</copyright>
    <facets>
        <!-- Don't document unsupported APIs -->
        <exclude name="http://kablink.org/facets#unsupported"/>
    </facets>
