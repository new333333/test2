        <java-json-client disabled="true"/>
        <java-xml-client disabled="true"/>
        <jackson1 disabled="true"/>
        <jackson honorJaxb="true" datatype-detection="passive"/>
        <jaxrs groupBy="annotation"/>
    </modules>
</enunciate>