<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" table="SS_BinderState" name="org.kablink.teaming.domain.BinderState">
    <id unsaved-value="null" name="binderId">
      <generator class="assigned"/>
    </id>
    <property name="zoneId" access="field"/>
    <property name="lastSyncTime"/>
    <property name="lastFullSyncCompletionTime"/>
    <property name="fullSyncStopRequested"/>
    <component name="fullSyncStats" class="org.kablink.teaming.domain.BinderState$FullSyncStats">
      <property name="statusStr" column="fullSync_status" length="16"/>
      <property name="startDate"/>
      <property name="endDate"/>
      <property name="dirOnly"/>
      <property name="enumerationFailed"/>
      <property name="countFiles"/>
      <property name="countFileAdd"/>
      <property name="countFileExpunge"/>
      <property name="countFileModify"/>
      <property name="countFileSetAcl"/>
      <property name="countFileSetOwnership" column="fullSync_countFileSetOwner"/>
      <property name="countFolders"/>
      <property name="countFolderAdd"/>
      <property name="countFolderExpunge"/>
      <property name="countFolderSetAcl"/>
      <property name="countFolderSetOwnership" column="fullSync_countFolderSetOwner"/>
      <property name="countEntryExpunge"/>
      <property name="countFailure"/>
      <property name="countFolderProcessed"/>
      <property name="countFolderMaxQueue"/>
    </component>
  </class>
</hibernate-mapping>
