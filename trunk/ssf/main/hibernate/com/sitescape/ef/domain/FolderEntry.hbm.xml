<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" lazy="true" table="SS_FolderEntries" name="com.sitescape.ef.domain.FolderEntry">
    <id unsaved-value="null" name="id" type="long" node="@id">
      <generator class="native"/>
    </id>
    <version unsaved-value="undefined" name="lockVersion" column="lockVersion" type="long"/>
    <map inverse="true" access="field" cascade="all,delete-orphan" lazy="true" name="customAttributes">
      <key column="folderEntry"/>
      <map-key type="string" column="name"/>
      <one-to-many class="com.sitescape.ef.domain.CustomAttribute"/>
    </map>
    <set inverse="true" access="field" cascade="all,delete-orphan" lazy="true" batch-size="4" name="attachments">
      <key column="folderEntry"/>
      <one-to-many class="com.sitescape.ef.domain.Attachment"/>
    </set>
    <set inverse="true" access="field" cascade="all,delete-orphan" lazy="true" batch-size="4" name="events">
      <key column="folderEntry"/>
      <one-to-many class="com.sitescape.ef.domain.Event"/>
    </set>
    <bag inverse="true" access="field" cascade="all,delete-orphan" optimistic-lock="false" lazy="true" name="replies">
      <key column="parentEntry"/>
      <one-to-many class="com.sitescape.ef.domain.FolderEntry"/>
    </bag>
    <property name="docContent"/>
    <property name="allowEdits"/>
    <component name="reservedDoc" class="com.sitescape.ef.domain.HistoryStamp">
      <property name="date" column="reserved_date" type="timestamp"/>
      <many-to-one class="com.sitescape.ef.domain.Principal" embed-xml="false" node="Principal" name="principal">
        <column name="reserved_principal"/>
      </many-to-one>
    </component>
    <property name="sendMail"/>
    <set inverse="true" access="field" cascade="all,delete-orphan" lazy="true" batch-size="4" name="workflowStates">
      <key column="folderEntry"/>
      <one-to-many class="com.sitescape.ef.domain.WorkflowState"/>
    </set>
    <component name="HKey" class="com.sitescape.ef.domain.HKey">
      <property name="level" column="entry_level"/>
      <property name="sortKey" length="512" column="entry_sortKey"/>
    </component>
    <property name="owningFolderSortKey" length="512"/>
    <property name="lastActivity"/>
    <property name="replyCount" not-null="true"/>
    <property name="nextDescendant" not-null="true"/>
    <property name="totalReplyCount" not-null="true"/>
    <many-to-one class="com.sitescape.ef.domain.FolderEntry" name="topEntry"/>
    <many-to-one class="com.sitescape.ef.domain.FolderEntry" name="parentEntry"/>
    <component name="workflowChange" class="com.sitescape.ef.domain.HistoryStamp">
      <property name="date" column="wrk_date" type="timestamp"/>
      <many-to-one class="com.sitescape.ef.domain.Principal" embed-xml="false" node="Principal" name="principal">
        <column name="wrk_principal"/>
      </many-to-one>
    </component>
    <many-to-one name="parentBinder"/>
    <component name="description">
      <property name="HDescription" column="description_text" type="com.sitescape.ef.dao.util.SSClobStringType"/>
      <property name="HFormat" column="description_format"/>
    </component>
    <property name="title" length="1024"/>
    <many-to-one access="field" class="com.sitescape.ef.domain.Definition" name="entryDef">
      <column name="entryDef" sql-type="char(32)"/>
    </many-to-one>
    <component name="creation" node="creation" class="com.sitescape.ef.domain.HistoryStamp">
      <property name="date" column="creation_date" type="timestamp"/>
      <many-to-one class="com.sitescape.ef.domain.Principal" embed-xml="false" node="Principal" name="principal">
        <column name="creation_principal"/>
      </many-to-one>
    </component>
    <component name="modification" node="modification" class="com.sitescape.ef.domain.HistoryStamp">
      <property name="date" column="modification_date" type="timestamp"/>
      <many-to-one class="com.sitescape.ef.domain.Principal" embed-xml="false" node="Principal" name="principal">
        <column name="modification_principal"/>
      </many-to-one>
    </component>
  </class>
</hibernate-mapping>
