<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" table="SS_Postings" name="com.sitescape.ef.domain.PostingDef">
    <id unsaved-value="null" name="id" node="@id">
      <column name="id" sql-type="char(32)"/>
      <generator class="uuid.hex"/>
    </id>
    <version unsaved-value="undefined" name="lockVersion" column="lockVersion" type="long"/>
    <property name="enabled"/>
    <property name="zoneName" length="100" not-null="true"/>
    <many-to-one name="binder"/>
    <many-to-one name="definition">
        <column name="definition" sql-type="char(32)"/>
    </many-to-one>
    <many-to-one name="emailAlias">
       <column name="emailAlias" sql-type="char(32)"/>
    </many-to-one>
    <property name="subject" length="256"/>
    <property name="replyPostingOption"/>
  </class>
</hibernate-mapping>
