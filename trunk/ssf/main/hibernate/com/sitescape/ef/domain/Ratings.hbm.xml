<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class table="SS_Ratings" name="com.sitescape.ef.domain.Rating">
    <composite-id name="id" class="com.sitescape.ef.domain.UserEntityPK">
      <key-property name="principalId"/>
      <key-property name="entityId"/>
      <key-property name="entityType"/>
    </composite-id>
    <version unsaved-value="undefined" name="lockVersion" column="lockVersion" type="long"/>
   <property name="rating"/>
  </class>
</hibernate-mapping>
