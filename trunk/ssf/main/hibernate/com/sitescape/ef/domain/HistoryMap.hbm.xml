<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" table="SS_HistoryMap" name="com.sitescape.team.domain.HistoryMap">
    <composite-id name="id" class="com.sitescape.team.domain.UserPerFolderPK">
      <key-property name="principalId"/>
      <key-property name="binderId"/>
    </composite-id>
    <property name="historyMap" not-null="true" type="org.springframework.orm.hibernate3.support.BlobSerializableType"/>
    <property name="lastPrune"/>
  </class>
</hibernate-mapping>
