<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class discriminator-value="N" dynamic-update="true" lazy="false" table="SS_Notifications" name="com.sitescape.ef.domain.Notification">
    <id unsaved-value="null" name="id" node="@id">
      <column name="id" sql-type="char(32)"/>
      <generator class="uuid.hex"/>
    </id>
    <discriminator type="string" column="type" length="1"/>
    <version unsaved-value="undefined" name="lockVersion" column="lockVersion" type="long"/>
    <property name="type" length="1" update="false" insert="false"/>
    <many-to-one name="binder"/>
    <many-to-one name="sendTo"/>
    <subclass name="com.sitescape.ef.domain.UserNotification" discriminator-value="U" dynamic-update="true">
      <property name="style"/>
      <property name="disabled"/>
    </subclass>
  </class>
</hibernate-mapping>
