<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
    <class discriminator-value="N" dynamic-update="true" table="SS_TokenInfo" lazy="false" name="com.sitescape.team.security.accesstoken.impl.TokenInfo">
    	<cache usage="read-write"/>
	    <id unsaved-value="null" name="id">
	      <column name="id" sql-type="char(32)"/>
	      <generator class="uuid.hex"/>
	    </id>	    
    	<discriminator type="char" column="type"/>
    	<property name="zoneId" access="field"/>
    	<property name="seed" length="128"/>

		<subclass name="com.sitescape.team.security.accesstoken.impl.TokenInfoSession" discriminator-value="S">
    		<property name="userId" index="userId_tokenInfoSession"/>
		</subclass>
    	
		<subclass name="com.sitescape.team.security.accesstoken.impl.TokenInfoRequest" discriminator-value="R">
	    	<property name="applicationId"/>
	    	<property name="userId"/>
	    	<property name="binderId"/>
	    	<property name="binderAccessConstraints" type="integer" access="field"/>
	    </subclass>
	</class>
</hibernate-mapping>