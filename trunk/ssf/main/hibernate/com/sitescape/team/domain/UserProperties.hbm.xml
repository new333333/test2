<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" table="SS_UserProperties" name="com.sitescape.team.domain.UserProperties">
    <cache usage="read-write"/>
    <composite-id name="id" class="com.sitescape.team.domain.UserPropertiesPK">
      <key-property name="principalId"/>
      <key-property name="binderId"/>
    </composite-id>
    <property name="zoneId" access="field"/>
    <property name="properties" not-null="true" type="org.springframework.orm.hibernate3.support.BlobSerializableType"/>
  </class>
</hibernate-mapping>
