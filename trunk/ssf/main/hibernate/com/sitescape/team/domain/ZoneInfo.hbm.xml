<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class table="SS_ZoneInfo" name="com.sitescape.team.domain.ZoneInfo">
    <cache usage="read-write"/>
    <id unsaved-value="null" name="id">
      <column name="id" sql-type="char(32)"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="zoneId" unique="true" not-null="true"/>
    <property name="zoneName" length="128" unique="true" not-null="true"/>
    <property name="virtualHost" length="255" unique="true"/>
  </class>
</hibernate-mapping>