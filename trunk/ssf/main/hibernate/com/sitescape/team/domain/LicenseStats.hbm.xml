<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class table="SS_LicenseStats" name="com.sitescape.team.domain.LicenseStats">
    <id unsaved-value="null" name="id">
      <column name="id" sql-type="char(32)"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="zoneId" not-null="true"/>
    <property name="snapshotDate"/>
    <property name="internalUserCount"/>
    <property name="externalUserCount"/>
 	<property name="checksum"/>
  </class>
</hibernate-mapping>