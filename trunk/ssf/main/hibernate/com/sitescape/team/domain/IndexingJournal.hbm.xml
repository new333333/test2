<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" table="SS_IndexingJournal" name="com.sitescape.team.domain.IndexingJournal">
    <id unsaved-value="null" name="id" type="long" node="@id">
      <generator class="native">
      	<param name="sequence">ss_indexingjournal_id_sequence</param>
      </generator>
    </id>
    <property name="zoneId" access="field"/>
    <property name="nodeId" length="128" index="indexingJournal_nodeIndex"/>
    <property name="indexName" length="160" index="indexingJournal_nodeIndex"/>
    <property name="operationName" length="32"/>
    <property name="operationArgs" length="20000000" type="org.springframework.orm.hibernate3.support.BlobSerializableType"/>
  </class>
</hibernate-mapping>
