<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:s="http://www.springframework.org/schema/security"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
    http://www.springframework.org/schema/security http://www.springframework.org/schema/security/spring-security-2.0.1.xsd">

	<bean id="authenticationModule" parent="baseTransactionProxy">
		<property name="target">
			<bean parent="dependOnCommonServicesTemplate" class="com.sitescape.team.module.authentication.impl.BaseAuthenticationModule">
				<property name="zoneModule"><ref bean="zoneModule"/></property>
			</bean>
		</property>
	</bean>

</beans>
