<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>installServlet</servlet-name>
    <servlet-class>org.kabling.teaming.install.server.InstallServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>installServlet</servlet-name>
    <url-pattern>/InstallConfig/install</url-pattern>
  </servlet-mapping>
  
   <servlet>
       <servlet-name>fileUploadServlet</servlet-name>
       <servlet-class>org.kabling.teaming.install.server.ConfigImportExportServlet</servlet-class>
   </servlet>

   <servlet-mapping>
       <servlet-name>fileUploadServlet</servlet-name>
       <url-pattern>/InstallConfig/fileUpload</url-pattern>
   </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
