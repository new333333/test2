<?xml version="1.0" encoding="UTF-8" ?>
<zoneConfiguration>
<!-- keep as separate file - only need to load once -->
	<definitionFile>config/definitions/_user.xml</definitionFile>		
	<definitionFile>config/definitions/_blogEntry.xml</definitionFile>		
	<definitionFile>config/definitions/_comment.xml</definitionFile>		
	<definitionFile>config/definitions/_calendarEntry.xml</definitionFile>		
	<definitionFile>config/definitions/_discussionEntry.xml</definitionFile>		
	<definitionFile>config/definitions/_discussionReply.xml</definitionFile>		
	<definitionFile>config/definitions/_libraryEntry.xml</definitionFile>		
	<definitionFile>config/definitions/_guestbookEntry.xml</definitionFile>		
	<definitionFile>config/definitions/_photoEntry.xml</definitionFile>		
	<definitionFile>config/definitions/_wikiEntry.xml</definitionFile>		
	<definitionFile>config/definitions/_blogFolder.xml</definitionFile>
	<definitionFile>config/definitions/_calendarFolder.xml</definitionFile>
	<definitionFile>config/definitions/_discussionFolderList.xml</definitionFile>
	<definitionFile>config/definitions/_discussionFolderTable.xml</definitionFile>
	<definitionFile>config/definitions/_libraryFolder.xml</definitionFile>
	<definitionFile>config/definitions/_guestbookFolder.xml</definitionFile>
	<definitionFile>config/definitions/_photoFolder.xml</definitionFile>
	<definitionFile>config/definitions/_wikiFolder.xml</definitionFile>
	<definitionFile>config/definitions/_workspace.xml</definitionFile>
	<definitionFile>config/definitions/_userWorkspace.xml</definitionFile>
	<template type="5">
		<property name="templateTitle">__configuration_default_folder</property>
		<property name="templateDescription">__configuration_default_folder_description</property>
		<property name="name">_discussionFolderConfig</property>
		<property name="entryDef">__discussionFolderList</property>
		<definition name="_discussionFolderList"/>
		<definition name="_discussionFolderTable"/>
		<definition name="_blogFolder"/>
		<definition name="_calendarFolder"/>
		<definition name="_wikiFolder"/>
		<definition name="_discussionEntry"/>				
	</template>
	
	<template type="5">
		<property name="templateTitle">__configuration_default_folder_blog</property>
		<property name="templateDescription">__configuration_default_folder_blog_description</property>
		<property name="name">_blogFolderConfig</property>
		<property name="entryDef">_blogFolder</property>
		<definition name="_blogFolder"/>
		<definition name="_calendarFolder"/>
		<definition name="_discussionFolderList"/>
		<definition name="_discussionFolderTable"/>
		<definition name="_wikiFolder"/>
		<definition name="_blogEntry"/>		
	</template>
	
	<template type="5">
		<property name="templateTitle">__configuration_default_folder_wiki</property>
		<property name="templateDescription">__configuration_default_folder_wiki_description</property>
		<property name="name">_wikiFolderConfig</property>
		<property name="entryDef">_wikiFolder</property>
		<property name="uniqueTitles">true</property>
		<definition name="_wikiFolder"/>
		<definition name="_blogFolder"/>
		<definition name="_calendarFolder"/>
		<definition name="_discussionFolderList"/>
		<definition name="_discussionFolderTable"/>
		<definition name="_wikiEntry"/>
	</template>
	
	<template type="5">
		<property name="templateTitle">__configuration_default_folder_calendar</property>
		<property name="templateDescription">__configuration_default_folder_calendar_description</property>
		<property name="name">_calendarFolderConfig</property>
		<property name="entryDef">_calendarFolder</property>
		<definition name="_calendarFolder"/>
		<definition name="_blogFolder"/>
		<definition name="_discussionFolderList"/>
		<definition name="_discussionFolderTable"/>
		<definition name="_wikiFolder"/>
		<definition name="_calendarEntry"/>	
	</template>
	
	<template type="5">
		<property name="templateTitle">__configuration_default_folder_library</property>
		<property name="templateDescription">__configuration_default_folder_library_description</property>
		<property name="library">true</property>
		<property name="name">_libraryFolderConfig</property>
		<property name="entryDef">_libraryFolder</property>
		<definition name="_libraryFolder"/>
		<definition name="_calendarFolder"/>
		<definition name="_libraryEntry"/>
	</template>
	
	<template type="5">
		<property name="templateTitle">__configuration_default_folder_guestbook</property>
		<property name="templateDescription">__configuration_default_folder_guestbook_description</property>
		<property name="name">_guestbookFolderConfig</property>
		<property name="entryDef">_guestbookFolder</property>
		<definition name="_guestbookFolder"/>
		<definition name="_guestbookEntry"/>
	</template>
	
	<template type="5">
		<property name="templateTitle">__configuration_default_folder_photo</property>
		<property name="templateDescription">__configuration_default_folder_photo_description</property>
		<property name="name">_photoFolderConfig</property>
		<property name="entryDef">_photoFolder</property>
		<definition name="_photoFolder"/>
		<definition name="_photoEntry"/>
	</template>
	
	<template type="8">
		<property name="templateTitle">__configuration_default_workspace</property>
		<property name="templateDescription">__configuration_default_workspace_description</property>
		<property name="name">_workspaceConfig</property>
		<property name="entryDef">_workspace</property>
		<definition name="_workspace"/>
	</template>
	
	<template type="12">
		<property name="templateTitle">__configuration_default_user_workspace</property>
		<property name="templateDescription">__configuration_default_user_workspace_description</property>
		<property name="name">_userWorkspaceConfig</property>
		<property name="entryDef">_userWorkspace</property>
		<definition name="_userWorkspace"/>
		<property name="template">_blogFolderConfig</property>
		<property name="template">_calendarFolderConfig</property>
		<property name="template">_libraryFolderConfig</property>
		<property name="template">_guestbookFolderConfig</property>
		<property name="template">_photoFolderConfig</property>
	</template>
	
</zoneConfiguration>