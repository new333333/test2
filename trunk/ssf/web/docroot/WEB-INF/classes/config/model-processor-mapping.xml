<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE model-processor-mapping SYSTEM "../../../resources/model-processor-mapping-1_0_0.dtd">

<model-processor-mapping>
	<processors>
		<processor class="com.sitescape.ef.module.mail.impl.DefaultFolderEmailFormatter" spring-bean-type="internal" spring-bean-name="defaultFolderEmailFormatter"/>
		<processor class="com.sitescape.ef.module.test.impl.CrazyForumAbstractComposer" spring-bean-type="none"/>
		<processor class="com.sitescape.ef.module.test.impl.ForumProcessorWithDependencyInjectionImpl" spring-bean-type="external" spring-bean-name="forumProcessorWithDependencyInjectionImpl"/>
		<processor class="com.sitescape.ef.jobs.DefaultEmailNotification" spring-bean-type="none"/>
		<processor class="com.sitescape.ef.jobs.DefaultLdapSynchronization" spring-bean-type="none"/>
		<processor class="com.sitescape.ef.jobs.DefaultFailedEmail" spring-bean-type="none"/>
		<processor class="com.sitescape.ef.module.folder.impl.DefaultFolderCoreProcessor" spring-bean-type="internal" spring-bean-name="defaultFolderCoreProcessor"/>
		<processor class="com.sitescape.ef.module.profile.impl.DefaultProfileCoreProcessor" spring-bean-type="internal" spring-bean-name="defaultProfileCoreProcessor"/>
	</processors>
	
	<default-mappings>
		<model-class name="com.sitescape.ef.domain.ProfileBinder">
			<mapping processor-key="processorKey_profileCoreProcessor" processor-class="com.sitescape.ef.module.profile.impl.DefaultProfileCoreProcessor"/>
		</model-class>
		<model-class name="com.sitescape.ef.domain.Folder">
			<mapping processor-key="processorKey_folderEmailFormatter" processor-class="com.sitescape.ef.module.mail.impl.DefaultFolderEmailFormatter"/>
			<mapping processor-key="processorKey_folderCoreProcessor" processor-class="com.sitescape.ef.module.folder.impl.DefaultFolderCoreProcessor"/>
			<mapping processor-key="processorKey_testForumAbstractComposer" processor-class="com.sitescape.ef.module.test.impl.CrazyForumAbstractComposer"/>
			<mapping processor-key="processorKey_testForumProcessorWithDependencyInjection" processor-class="com.sitescape.ef.module.test.impl.ForumProcessorWithDependencyInjectionImpl"/>
			<mapping processor-key="processorKey_emailNotificationJob" processor-class="com.sitescape.ef.jobs.DefaultEmailNotification"/>
			<mapping processor-key="processorKey_failedEmailJob" processor-class="com.sitescape.ef.jobs.DefaultFailedEmail"/>
		</model-class>
		<model-class name="com.sitescape.ef.domain.Workspace">
			<mapping processor-key="processorKey_wsLdapSynchronizationJob" processor-class="com.sitescape.ef.jobs.DefaultLdapSynchronization"/>
			<mapping processor-key="processorKey_wsLdapNameMapper" processor-class="com.sitescape.ef.module.ldap.DefaultLdapNameMapper"/>
		</model-class>
	</default-mappings>
</model-processor-mapping>