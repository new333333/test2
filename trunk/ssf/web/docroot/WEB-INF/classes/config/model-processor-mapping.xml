<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE model-processor-mapping SYSTEM "../../../resources/model-processor-mapping-1_0_0.dtd">

<model-processor-mapping>
	<processors>
		<processor class="com.sitescape.ef.mail.impl.DefaultFolderEmailFormatter" spring-bean-type="internal" spring-bean-name="defaultFolderEmailFormatter"/>
		<processor class="com.sitescape.ef.jobs.DefaultEmailNotification" spring-bean-type="none"/>
		<processor class="com.sitescape.ef.jobs.DefaultLdapSynchronization" spring-bean-type="none"/>
		<processor class="com.sitescape.ef.jobs.DefaultFailedEmail" spring-bean-type="none"/>
		<processor class="com.sitescape.ef.jobs.DefaultSendEmail" spring-bean-type="none"/>
		<processor class="com.sitescape.ef.module.folder.impl.DefaultFolderCoreProcessor" spring-bean-type="internal" spring-bean-name="defaultFolderCoreProcessor"/>
		<processor class="com.sitescape.ef.module.folder.impl.DefaultFileFolderCoreProcessor" spring-bean-type="internal" spring-bean-name="defaultFileFolderCoreProcessor"/>
		<processor class="com.sitescape.ef.module.profile.impl.DefaultProfileCoreProcessor" spring-bean-type="internal" spring-bean-name="defaultProfileCoreProcessor"/>
		<processor class="com.sitescape.ef.module.workspace.impl.DefaultWorkspaceCoreProcessor" spring-bean-type="internal" spring-bean-name="defaultWorkspaceCoreProcessor"/>
	</processors>
	
	<default-mappings>
		<model-class name="com.sitescape.ef.domain.ProfileBinder">
			<mapping processor-key="processorKey_binderCoreProcessor" processor-class="com.sitescape.ef.module.profile.impl.DefaultProfileCoreProcessor"/>
		</model-class>
		<model-class name="com.sitescape.ef.domain.Folder">
			<mapping processor-key="processorKey_folderEmailFormatter" processor-class="com.sitescape.ef.mail.impl.DefaultFolderEmailFormatter"/>
			<mapping processor-key="processorKey_binderCoreProcessor" processor-class="com.sitescape.ef.module.folder.impl.DefaultFolderCoreProcessor"/>
			<mapping processor-key="processorKey_binderCoreProcessor_5" processor-class="com.sitescape.ef.module.folder.impl.DefaultFolderCoreProcessor"/>
			<mapping processor-key="processorKey_binderCoreProcessor_9" processor-class="com.sitescape.ef.module.folder.impl.DefaultFileFolderCoreProcessor"/>
			<mapping processor-key="processorKey_emailNotificationJob" processor-class="com.sitescape.ef.jobs.DefaultEmailNotification"/>
			<mapping processor-key="processorKey_sendEmailJob" processor-class="com.sitescape.ef.jobs.DefaultSendEmail"/>
			<mapping processor-key="processorKey_failedEmailJob" processor-class="com.sitescape.ef.jobs.DefaultFailedEmail"/>
		</model-class>
		<model-class name="com.sitescape.ef.domain.Workspace">
			<mapping processor-key="processorKey_wsLdapSynchronizationJob" processor-class="com.sitescape.ef.jobs.DefaultLdapSynchronization"/>
			<mapping processor-key="processorKey_wsLdapNameMapper" processor-class="com.sitescape.ef.ldap.DefaultLdapNameMapper"/>
			<mapping processor-key="processorKey_binderCoreProcessor" processor-class="com.sitescape.ef.module.workspace.impl.DefaultWorkspaceCoreProcessor"/>
		</model-class>
	</default-mappings>
</model-processor-mapping>