<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" table="SS_SeenMap" name="com.sitescape.team.domain.SeenMap">
    <id name="principalId">
      <generator class="assigned"/>
    </id>
    <property name="zoneId" access="field"/>
<!-- use length to force medium blog in mysql -->
    <property name="seenMap" length="2000000" not-null="true" type="org.springframework.orm.hibernate3.support.BlobSerializableType"/>
    <property name="lastPrune"/>
  </class>
</hibernate-mapping>
