<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" lazy="false" table="SS_BinderQuota" name="org.kablink.teaming.domain.BinderQuota">
    <!--<cache usage="read-write"/>-->
    <id unsaved-value="null" name="binderId" type="long"/>
    <property name="zoneId" not-null="true"/>
    <property name="diskQuota"/>
    <property name="diskSpaceUsed"/>
    <property name="diskSpaceUsedCumulative"/>
  </class>
</hibernate-mapping>