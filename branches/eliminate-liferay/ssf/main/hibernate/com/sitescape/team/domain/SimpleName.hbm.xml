<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
    <class dynamic-update="true" table="SS_SimpleName" lazy="false" name="com.sitescape.team.domain.SimpleName">
    	<cache usage="nonstrict-read-write"/>
	    <composite-id>
	      <key-property name="zoneId" access="field"/>
	      <key-property name="name" length="128" access="field"/>
	    </composite-id>
	    <property name="emailAddress" length="128" access="field" index="emailAddress_simpleName"/>
    	<property name="binderId"/>
    	<property name="binderType" length="16"/>
	</class>
</hibernate-mapping>