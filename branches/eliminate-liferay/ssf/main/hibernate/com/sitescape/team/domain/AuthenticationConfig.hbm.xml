<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class table="SS_AuthenticationConfig" name="com.sitescape.team.domain.AuthenticationConfig">
    <id unsaved-value="null" name="id">
      <column name="id" sql-type="char(32)"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="zoneId" access="field"/>
    <property name="url"/>
    <property name="mappings" type="org.springframework.orm.hibernate3.support.ClobStringType"/>
    <property name="userSearches" type="org.springframework.orm.hibernate3.support.ClobStringType"/>
    <property name="position"/>
  </class>
</hibernate-mapping>