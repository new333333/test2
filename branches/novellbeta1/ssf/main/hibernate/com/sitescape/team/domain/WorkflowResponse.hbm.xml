<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class dynamic-update="true" lazy="false" table="SS_WorkflowResponses" name="com.sitescape.team.domain.WorkflowResponse">
    <id unsaved-value="null" name="id">
      <column name="id" sql-type="char(32)"/>
      <generator class="uuid.hex"/>
    </id>
    <component name="owner" class="com.sitescape.team.domain.AnyOwner">
      <many-to-one name="folderEntry" index="ownerFolderEntry"/>
      <many-to-one name="principal" index="ownerPrincipal"/>
      <many-to-one name="binder" index="ownerBinder"/>
      <any name="entity" id-type="java.lang.Long" meta-type="string" index="entityOwner">
        <meta-value class="com.sitescape.team.domain.FolderEntry" value="folderEntry"/>
        <meta-value class="com.sitescape.team.domain.User" value="user"/>
        <meta-value class="com.sitescape.team.domain.Group" value="group"/>
        <meta-value class="com.sitescape.team.domain.Folder" value="folder"/>
        <meta-value class="com.sitescape.team.domain.Workspace" value="workspace"/>
        <meta-value class="com.sitescape.team.domain.ProfileBinder" value="profileBinder"/>
        <meta-value class="com.sitescape.team.domain.TemplateBinder" value="template"/>
        <column name="ownerType" length="16"/>
        <column name="ownerId"/>
      </any>
      <property name="ownerType" update="false" insert="false"/>
      <property name="ownerId" update="false" insert="false"/>
      <property name="owningFolderSortKey" length="512"/>
      <property name="owningBinderId" index="owningBinder"/>
    </component>
    <property name="name" length="64"/>
    <property name="response" length="2000"/>
    <property name="definitionId" length="32"/>
    <property name="responderId"/>
    <property name="responseDate"/>
 </class>
</hibernate-mapping>
