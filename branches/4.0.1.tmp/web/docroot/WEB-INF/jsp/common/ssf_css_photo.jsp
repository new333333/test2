/* Photos Archive Column */

div.ss_photo_content {
	padding-bottom: 30px;
}
div.ss_photo_sidebar {
    background-color: #fff; 
    padding: 0px 5px 10px 5px;
    border-left: 1px solid #e0e0e0;
}
.ss_photo_sidebar_subhead {
	font-size: ${ss_style_font_largeprint} !important;
	padding-bottom: 3px;
}
.ss_photo_sidebar_box {
	margin-bottom: 8px;
	margin-top: 2px;
	padding: 2px 5px;
}
.ss_photo_archive_subhead {
	margin-left: 18px;
	font-weight: bold;
	font-size: ${ss_style_font_largeprint} !important;
	padding-top: 5px;
	padding-bottom: 5px;
}
.ss_photo_title  {
	font-weight: bold;
	font-size: ${ss_style_font_largeprint} !important;
	padding-bottom: 2px;

}
